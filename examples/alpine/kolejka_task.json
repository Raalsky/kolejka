{
  "id": "example_alpine",
  "image": "alpine:latest",
  "files": {
    "test.sh" : {
        "path" : "test.sh"
    }
  },
  "limits": {
    "gpus": 0,
    "gpu_memory": 0,
    "cpus": 1,
    "pids": 32,
    "memory": "64M",
    "storage": "64M",
    "image": "512M",
    "workspace": "64M",
    "time": 10
  },
  "args": [
    "./test.sh"
  ],
  "stderr": "console.txt",
  "stdout": "console.txt"
}
